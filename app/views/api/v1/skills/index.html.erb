<p id="notice"><%= notice %></p>

<h1>Listing Api V1 Skills</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_skills.each do |api_v1_skill| %>
      <tr>
        <td><%= link_to 'Show', api_v1_skill %></td>
        <td><%= link_to 'Edit', edit_api_v1_skill_path(api_v1_skill) %></td>
        <td><%= link_to 'Destroy', api_v1_skill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Skill', new_api_v1_skill_path %>
