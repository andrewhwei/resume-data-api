<p id="notice"><%= notice %></p>

<h1>Listing Api V1 Capstones</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_capstones.each do |api_v1_capstone| %>
      <tr>
        <td><%= link_to 'Show', api_v1_capstone %></td>
        <td><%= link_to 'Edit', edit_api_v1_capstone_path(api_v1_capstone) %></td>
        <td><%= link_to 'Destroy', api_v1_capstone, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

